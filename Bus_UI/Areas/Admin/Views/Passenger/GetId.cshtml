@model PassengersListView
@{
    ViewData["Title"] = "GetId";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
  <link rel="stylesheet" href="~/css/to-go.css">
  <h4 class="text-center">Passengers GetId</h4>
 <div class="row">
					<div class="col-md-12">
						<div class="search-wrap-1 ftco-animate">
		        		<div class="row">
		        			<div class="col-lg align-items-end">
		        				<div class="form-group">
		        					<label for="#">Haradan</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
		                     <input type="text" class="form-control" value="@Model.FromWhere" name="FromWhere" id="fromwhere" placeholder="şəhər əlavə edin" disabled>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        			<div class="col-lg align-items-end">
		        				<div class="form-group">
		        					<label for="#">Haraya</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
                          <input type="text" class="form-control" value="@Model.ToWhere" name="ToWhere" id="where" placeholder="şəhər əlavə edin" disabled>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        			<div class="col-lg align-items-end">
		        				<div class="form-group">
		        					<label for="#">Tarix</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
		               
		              <input type="date" id="Date" value="@Model.DepartureDate" name="DepartureDate" class="form-control" disabled>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        			<div class="col-lg align-items-end">
		        				<div class="form-group">
		        					<label for="#">Saat</label>
		        					<div class="form-field">
		          					<div class="select-wrap">
										<select name="DepartureHour" id="" class="form-control">
											<option value="@Model.DepartureHour">@Model.DepartureHour</option>
										  
										</select>
		                    </div>
				              </div>
			              </div>
		        			</div>
		        			<div class="col-lg align-self-end">
		        				<div class="form-group">
		        					<div class="form-field">
										<button class="form-control btn btn-primary"> Axtarin</button>
				              </div>
			              </div>
		        			</div>
		        		</div>
		        		</div>
		        		</div>
		        
		        </div>


<div class="form-group">
	<label>Full Name</label>
	<label class="form-control">
		@Model.FullName
	</label>
</div>
<div class="form-group">
	<label>Fin Code</label>
	<label class="form-control">
		@Model.IdentityNumber
	</label>
</div>
<div class="form-group">
	<label>Seat</label>
	<label class="form-control">
		@Model.Seat
	</label>
</div>
<div class="form-group">
	<label>Email</label>
	<label class="form-control">
		@Model.Email
	</label>
</div>

<div class="form-group">
	<label>Phone</label>
	<label class="form-control">
		@Model.Phone
	</label>
</div>
<div class="form-group">
	<label>PostingDate</label>
	<label class="form-control">
		@Model.PostingDate
	</label>
</div>


<script src="https://code.jquery.com/jquery-3.7.0.js" integrity="sha256-JlqSTELeR4TLqP0OG9dxM7yDPqX1ox/HfgiSLBj8+kM=" crossorigin="anonymous"></script>
		<script src="~/lib/jquery/dist/jquery.js"></script>		
